<h3>可用的type id</h3>
<% @types.each do |type| %>
  <li>
    <%= type.name %>
    <%= type.id %>
  </li>
<% end %>

<h3>創造新style</h3>
<%= form_for [:admin,@style] do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :type_id, "type_id" %>
  <%= f.text_field :type_id %>
  <%= f.submit %>
<% end %>